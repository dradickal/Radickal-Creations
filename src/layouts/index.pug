extends layout.pug

block navLinks
    a(href="#skills") Skills
    a(href="#contact") Contact

block body
    header
        h1 #{model.header.title}
        p #{model.header.content}
    content
        div.hero

        div#skills
            each topic, index in model.topics
                - var highlight= model.highlights[index];
                include partials/topic.pug               
                include partials/highlight.pug
                    

